type: log
paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]
processors:
  - script:
      lang: javascript
      id: googlecloud_audit_script
      file: ${path.home}/module/googlecloud/audit/config/pipeline.js
      params:
        keep_original_message: {{keep_original_message}}