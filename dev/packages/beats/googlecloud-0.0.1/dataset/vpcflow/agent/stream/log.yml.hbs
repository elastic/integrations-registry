type: log
paths:
{{#each paths as |path|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]
processors:
  - script:
      lang: javascript
      id: googlecloud_vpcflow_script
      file: ${path.home}/module/googlecloud/vpcflow/config/pipeline.js
      params:
        keep_original_message: {{keep_original_message}}