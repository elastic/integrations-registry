type: log
paths:
{{#each paths as |path|}}
 - {{path}}
{{/each}}
exclude_files: [".gz$"]
tags: {{tags}}
json.keys_under_root: false
fields_under_root: true
processors:
  - rename:
      fields:
        - from: "json"
          to: "zeek.capture_loss"
      ignore_missing: true
      fail_on_error: false